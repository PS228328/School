<Window x:Class="WpfAppEindopdrachtDD2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEindopdrachtDD2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock FontSize="30" FontWeight="ExtraBlack" Margin="0,0,0,0" Background="Bisque" Padding="20,10,20,10">Selecteer wat je wil doen</TextBlock>
            <Button x:Name="btnBinnenwedstrijd" Click="btnBinnenwedstrijd_Click">Binnenwedstrijd invoegen</Button>
            <Button x:Name="btnBuitenwedstrijd" Click="btnBuitenwedstrijd_Click">Buitenwedstrijd invoegen</Button>
            <Button x:Name="btnBioscoopuitje" Click="btnBioscoopuitje_Click">Bioscoopuitje invoegen</Button>
            <Button x:Name="btnMuseumuitje" Click="btnMuseumuitje_Click">Museumuitje invoegen</Button>
        </StackPanel>
    </Grid>
</Window>
