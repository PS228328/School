<Window x:Class="Eindopdracht_WPF_PRG2.Hurray"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Eindopdracht_WPF_PRG2"
        mc:Ignorable="d"
        Title="Hurray" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Assets/Hurray.png"></ImageBrush>
        </Grid.Background>
        <TextBlock x:Name="countdowner" Text="10" HorizontalAlignment="Center" FontSize="60" FontWeight="Bold"></TextBlock>
        <TextBlock x:Name="win" Text="Je hebt het spel gewonnen in ? seconde" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="35" FontWeight="Bold"></TextBlock>
    </Grid>
</Window>
