<Window x:Class="_5_okt_2021_Switch_statements.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_5_okt_2021_Switch_statements"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Width="300" Margin="0, 100, 0,0">
            <TextBox x:Name="tbInvoer" ></TextBox>
            <Button x:Name="btGo" Content="Go!" Click="btGo_Click"></Button>
            <Button x:Name="btSwitch" Content="Switch!" Click="btSwitch_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
