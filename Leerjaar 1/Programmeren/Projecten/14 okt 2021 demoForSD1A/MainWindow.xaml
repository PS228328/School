<Window x:Class="_14_okt_2021_demoForSD1A.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_14_okt_2021_demoForSD1A"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="LightGray">
        <StackPanel Width="350" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,50,0,50">
            <Button x:Name="btGo" Background="Blue" Foreground="White" Content="Go" Width="150" Click="BtGo_Click"></Button>
            <TextBlock x:Name="tbOutput" Text="0" Background="LightYellow" Width="200" TextAlignment="Center"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
