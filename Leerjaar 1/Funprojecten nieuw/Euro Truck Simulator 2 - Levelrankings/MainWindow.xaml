<Window x:Class="Euro_Truck_Simulator_2___Levelrankings.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Euro_Truck_Simulator_2___Levelrankings"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image Source="Fotoalbum/Euro Truck Simulator 2 Logo.jpg"></Image>
        <Button Background="Black" Width="150" Height="50" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,50,50,0" Foreground="White" Click="Button_Click">Bij welk level ben ik?</Button>
        <TextBox x:Name="levelchecker" Background="Black" Width="150" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,50,0,0" Foreground="White" TextAlignment="Center" FontSize="30" FontWeight="Bold">Vul hier in</TextBox>
    </Grid>
</Window>
